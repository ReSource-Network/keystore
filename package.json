{
  "name": "guardian",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "seed": "prisma db seed --preview-feature",
    "start": "node dist/index.js",
    "start:watch": "nodemon dist/index.js",
    "fetch-env": "bin/fetchEnv.sh keystore",
    "build": "tsc",
    "build:watch": "tsc -w",
    "compose:dev": "docker-compose up",
    "compose:local": "env-cmd -f .env.local docker-compose -f docker-compose.yml -f docker-compose.local.yml up",
    "prebuild": "yarn run clean && yarn run generate",
    "clean": "yarn rimraf dist",
    "generate": "yarn prisma generate",
    "db:seed": "yarn prisma db seed --preview-feature",
    "db:push": "yarn prisma db push",
    "db:save": "yarn prisma migrate dev --create-only --preview-feature",
    "db:migrate": "yarn prisma migrate dev --preview-feature",
    "db:migrate:prod": "yarn prisma migrate deploy --preview-feature",
    "prisma:studio": "yarn prisma studio",
    "docker:build": "yarn run build && docker build -t shortlink .",
    "prod:start": "yarn run db:migrate && yarn start",
    "test": "jest --detectOpenHandles --force-exit"
  },
